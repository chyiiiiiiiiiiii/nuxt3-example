{
  "compilerOptions": {
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "jsx": "preserve"
    // "types": ["@nuxtjs/axios", "@types/node", "@nuxtjs/i18n"]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
